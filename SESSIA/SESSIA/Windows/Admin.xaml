<Window x:Class="SESSIA.Windows.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SESSIA.Windows"
        mc:Ignorable="d"
        Title="Admin" MinHeight="720" MinWidth="1280"
        WindowStartupLocation="CenterOwner"
        WindowState="Maximized">
    <Grid>
        <Viewbox Margin="40">
            <StackPanel Height="100" Width="190" Background="#FF76E383">
                <ListView Height="90" Width="150">

                </ListView>
                <Button x:Name="Back_BTN" Content="Назад" Click="Back_BTN_Click" FontSize="5" FontFamily="Comic Sans MS" ToolTip="Вернуться на прошлую страницу" Height="10" Background="#FF498C51" Margin="75,0,74,0"/>

            </StackPanel>
        </Viewbox>
    </Grid>
</Window>
